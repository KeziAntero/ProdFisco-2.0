            "content": "import { useNavigate } from 'react-router-dom';\nimport { Button } from '@/components/ui/button';\nimport { Card, CardContent, CardDescription, CardHeader, CardTitle } from '@/components/ui/card';\nimport { Plus, List, BarChart3 } from 'lucide-react';\nimport { useAuth } from '@/hooks/useAuth';\n\nconst Index = () => {\n  const navigate = useNavigate();\n  const { user } = useAuth();\n\n  return (\n    <div className=\"space-y-6\">\n      <div className=\"flex items-center gap-4\">\n        <img src=\"/logo.svg\" alt=\"ProdFisco Logo\" className=\"w-16 h-16\" />\n        <div>\n          <h1 className=\"text-3xl font-bold\">Bem-vindo ao ProdFisco</h1>\n          <p className=\"text-muted-foreground\">Olá, {user?.email}! Gerencie seus registros de produtividade fiscal.</p>\n        </div>\n      </div>\n\n      <div className=\"grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6\">\n        <Card className=\"hover:shadow-lg transition-shadow cursor-pointer\" onClick={() => navigate('/nova-prt')}>\n          <CardHeader>\n            <CardTitle className=\"flex items-center gap-2\">\n              <Plus className=\"h-5 w-5\" />\n              Nova PRT\n            </CardTitle>\n            <CardDescription>\n              Criar um novo registro de produtividade\n            </CardDescription>\n          </CardHeader>\n          <CardContent>\n            <Button className=\"w-full\">\n              Criar Registro\n            </Button>\n          </CardContent>\n        </Card>\n\n        <Card className=\"hover:shadow-lg transition-shadow cursor-pointer\" onClick={() => navigate('/lista')}>\n          <CardHeader>\n            <CardTitle className=\"flex items-center gap-2\">\n              <List className=\"h-5 w-5\" />\n              Lista de Produtividade\n            </CardTitle>\n            <CardDescription>\n              Visualizar histórico de registros\n            </CardDescription>\n          </CardHeader>\n          <CardContent>\n            <Button variant=\"outline\" className=\"w-full\">\n              Ver Histórico\n            </Button>\n          </CardContent>\n        </Card>\n\n        <Card className=\"hover:shadow-lg transition-shadow\">\n          <CardHeader>\n            <CardTitle className=\"flex items-center gap-2\">\n              <BarChart3 className=\"h-5 w-5\" />\n              Relatórios\n            </CardTitle>\n            <CardDescription>\n              Gerar relatórios de produtividade\n            </CardDescription>\n          </CardHeader>\n          <CardContent>\n            <Button variant=\"outline\" className=\"w-full\" disabled>\n              Em breve\n            </Button>\n          </CardContent>\n        </Card>\n      </div>\n\n      <Card>\n        <CardHeader>\n          <CardTitle>Como usar o sistema</CardTitle>\n        </CardHeader>\n        <CardContent className=\"space-y-4\">\n          <div className=\"grid grid-cols-1 md:grid-cols-2 gap-4\">\n            <div>\n              <h3 className=\"font-semibold mb-2\">1. Criar uma nova PRT</h3>\n              <p className=\"text-sm text-muted-foreground\">\n                Registre suas atividades de fiscalização com os serviços realizados, \n                quantidade de fiscais envolvidos e documentos relacionados.\n              </p>\n            </div>\n            <div>\n              <h3 className=\"font-semibold mb-2\">2. Visualizar histórico</h3>\n              <p className=\"text-sm text-muted-foreground\">\n                Consulte todos os seus registros por período, edite ou exclua \n                conforme necessário.\n              </p>\n            </div>\n            <div>\n              <h3 className=\"font-semibold mb-2\">3. Cálculo automático</h3>\n              <p className=\"text-sm text-muted-foreground\">\n                O sistema calcula automaticamente a pontuação baseada no tipo \n                de serviço e quantidade de fiscais envolvidos.\n              </p>\n            </div>\n            <div>\n              <h3 className=\"font-semibold mb-2\">4. Relatórios</h3>\n              <p className=\"text-sm text-muted-foreground\">\n                Gere relatórios em PDF com o detalhamento completo dos \n                registros de produtividade.\n              </p>\n            </div>\n          </div>\n        </CardContent>\n      </Card>\n    </div>\n  );\n};\n\nexport default Index;\n"
